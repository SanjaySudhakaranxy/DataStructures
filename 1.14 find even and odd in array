#include<stdio.h>
int main()
{
   printf("find odd and even number in array\n");
   printf("TANUJA P ,192321052 \n");
   int n=6,i,flag=0;
   int arr[]={1,2,3,4,5,6};
   printf("even number in the array : \n");
   for(i=0;i<n;i++)
   {
     if(arr[i]%2==0)
       printf("%d ",arr[i]);
   }
   printf("\n");
   printf("Odd numbers in the array: ");
    for (i=0;i<n;i++)
    {
        if (arr[i]%2!=0)
        {
            printf("%d ", arr[i]);
        }
    }
}
